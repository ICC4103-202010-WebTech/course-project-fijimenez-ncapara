<p id="notice"><%= notice %></p>

<h1>Vote Dates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_dates.each do |vote_date| %>
      <tr>
        <td><%= link_to 'Show', vote_date %></td>
        <td><%= link_to 'Edit', edit_vote_date_path(vote_date) %></td>
        <td><%= link_to 'Destroy', vote_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote Date', new_vote_date_path %>
