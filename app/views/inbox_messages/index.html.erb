<p id="notice"><%= notice %></p>

<h1>Inbox Messages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inbox_messages.each do |inbox_message| %>
      <tr>
        <td><%= link_to 'Show', inbox_message %></td>
        <td><%= link_to 'Edit', edit_inbox_message_path(inbox_message) %></td>
        <td><%= link_to 'Destroy', inbox_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inbox Message', new_inbox_message_path %>
